{
  "productName": "YouTube TV",
  "version": "0.1.0",
  "identifier": "com.yttv.desktop",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../out"
  },
  "app": {
    "windows": [
      {
        "title": "YouTube TV",
        "width": 1280,
        "height": 800,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "nsis",
    "resources": [
      "../default-channels.json"
    ]
  }
}